{
  "scripts": {
    "clean": "rm -rf lib_es6 lib docs_src/webpack/*.js dist docs docs_ts_processed react-konva.d.ts storybook-built",
    "build_es6": "tsc --project tsconfig.lib.json",
    "es6_to_es5": "babel lib_es6 --out-dir lib",
    "build_lib": "npm run build_es6 && npm run es6_to_es5",
    "prepublish": "npm run clean && npm run build",
    "compile_doc_webpack": "node dts-generator.js && tsc docs_src/webpack/build.ts",
    "build": "npm run build_lib && npm run compile_doc_webpack && webpack --config docs_src/webpack/build.js",
    "run_doc": "npm run compile_doc_webpack && webpack-dev-server --config docs_src/webpack/build.js",
    "storybook": "tsc && start-storybook -c .storybook -p 7001 -s vs",
    "build_storybook": "tsc && build-storybook -c .storybook -o storybook-built",
    "test": "npm run unittest",
    "unittest": "jest --config src/__unittests__/jest_unittests.json"
  },
  "name": "reactive-konva",
